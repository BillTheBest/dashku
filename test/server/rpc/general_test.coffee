describe "General", ->

  describe "#getDocument", ->

    describe "if successful", ->
 
      it "should return a content object containing rendered markdown"

      it "should replace references to API_KEY with the user's api key"

      it "should replace references to DASHKU_API_URL with the api url for Dashku"

    describe "if not successful", ->

      it "should return a string saying 'Error accessing document'"